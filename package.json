{
  "name": "mango-test",
  "version": "1.0.0",
  "description": "mango test",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --cache --fix \"{src,apps}/**/*.ts\" ",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./migrations/dev --dataSource ./data-source.ts -p",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create ./migrations/dev",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource ./data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --dataSource ./data-source.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fastify/static": "6.10.2",
    "@fastify/view": "7.4.1",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/microservices": "10.2.6",
    "@nestjs/platform-express": "10.1.0",
    "@nestjs/platform-fastify": "10.1.0",
    "@nestjs/swagger": "7.0.5",
    "@nestjs/typeorm": "10.0.0",
    "change-case": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "2.29.3",
    "handlebars": "4.7.7",
    "husky": "8.0.3",
    "ioredis": "^5.3.2",
    "nest-winston": "1.9.4",
    "nestjs-real-ip": "3.0.1",
    "pg": "8.10.0",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "swagger-stats": "0.95.9",
    "timezone-enum": "1.0.4",
    "typeorm": "0.3.17",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/express": "4.17.17",
    "@types/node": "16.18.25",
    "@types/supertest": "2.0.12",
    "@types/swagger-stats": "0.95.9",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "cli-progress": "3.12.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "47.0.0",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "5.1.6"
  }
}
